    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".AndroidApplication"
        android:label="@string/app_name"
        android:theme="@style/Base.Theme.OompaLoompaTest">
        <activity
            android:name="franug.oompaloompatest.view.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="franug.oompaloompatest.view.DetailActivity"
            android:exported="true">
        </activity>
    </application>

</manifest>